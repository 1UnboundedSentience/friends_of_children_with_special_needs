<h1>Class selections</h1>
<table class="table">
  <thead>
    <th>Child name</th>
    <th>Class name</th>
    <th>Photo Waiver</th>
    <th>Status</th>
  </thead>
  <tbody>
    <% for registration in @registrations %>
      <td><%=registration.student.person.display_name%></td>
      <td><a href="/home/<%=registration.course.id%>"><%=registration.course.name%></a>
      </td>
      <td><%=registration.photo_waiver%></td>
      <td><%=registration.status%></td>
    <%end%>
  </tbody>
</table>
