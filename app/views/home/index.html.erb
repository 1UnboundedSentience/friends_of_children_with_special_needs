<h1>Class selections</h1>
<table class="table table-striped">
  <thead>
    <th>Child name</th>
    <th>Class name</th>
    <th>Photo Waiver</th>
    <th>Status</th>
  </thead>
  <tbody>
    <% for registration_item in @registration_items %>
      <tr>
        <td><%=registration_item.student.person.display_name%></td>
        <td><a href="/home/<%=registration_item.course.id%>"><%=registration_item.course.name%></a>
        </td>
        <td><%= @registration.photo_waiver %></td>
        <td><%= @registration.status %></td>
      </tr>
    <%end%>
  </tbody>
</table>